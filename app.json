    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.live.freedom",
      "buildNumber": "7"
    },
    "android": {
      "package": "com.live.freedom",
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#3182ce"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
