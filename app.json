{
  "expo": {
    "name": "LIVE Freedom | App",
    "slug": "live-freedom-app",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3182ce"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
